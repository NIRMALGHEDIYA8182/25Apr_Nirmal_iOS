#include <stdio.h>

// Function to calculate the surface area of a rectangular prism
double calculateSurfaceArea(double width, double length, double height) {
    double surfaceArea = 2 * (width * length + height * length + height * width);
    return surfaceArea;
}

int main() {
    // Example values for width, length, and height
    double width = 3.0;
    double length = 4.0;
    double height = 5.0;

    // Calculate the surface area using the function
    double area = calculateSurfaceArea(width, length, height);

    // Display the result
    printf("Surface Area: %.2f\n", area);

    return 0;
}
